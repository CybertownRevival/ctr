DEF bgrounder Script{
eventIn SFFloat cl
eventOut MFColor bcolor
eventOut SFColor starlight
eventOut SFVec3f stars
eventOut SFFloat  cricket_changed
field SFFloat star 0
field SFFloat timer 0
url "vrmlscript:
function cl( timex ){
 timer = (Math.sin( timex*6.28 ));
 if (timer <.5){timer *= .9;}
 if(timer<0){timer=0;}
 bcolor [0] = new SFColor(0,0,timer);
 bcolor [1] = new SFColor(0,.5*timer,timer);
 bcolor [2] = new SFColor(timer,timer,timer); 
 if(timex >=.48){
   stars = new SFVec3f(100,100,100);
   cricket_changed = 1;
   if(timex <= .58){star = (timex - 0.48)*10;}
   else{star = 1;}
   if(timex >= .9){star = (.1-(timex - .9))*10;}
   if(star > 1){star = 1;}        
 }
 if (timex < .48){
        cricket_changed = 0;
        star = 0;
        stars = new SFVec3f(.001,.001,.001);
 }
 starlight = new SFColor(star,star,star);
}
"}

]}
]}




