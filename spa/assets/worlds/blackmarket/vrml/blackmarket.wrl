url "vrmlscript:
function onEnterTime(val,ts) {
Browser.setNavigationMode('NONE');
if(isPlace){
u = new MFString('javascript:changeFrames(\"' + menu_url_string + '\",\"' + action_url_string + '\")');
param = new MFString('target=action');
Browser.loadURL(u,param);     
Browser.loadURL(bxx_url_string,bxx_param_string); 
} 
else{
Browser.loadURL(bxx_url_string,bxx_param_string); 
}
}
"
}
